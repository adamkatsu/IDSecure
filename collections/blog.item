<section class="blog-return">
  <div class="container blog-return-cont">
    <a href="/">
      <button class="btn-return">
        <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
          <path d="M15 18L9 12L15 6" stroke="white" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
        </svg>
        <span>Back</span>
      </button>
    </a>
  </div>
</section>

{.section item}

<section class="blog-head">
  <div class="container blog-head-cont">
    <img src="{assetUrl}" alt="">
  </div>
</section>

<section class="blog-main">
  <div class="container blog-main-cont">
    <div class="blog-main-title">
      <span class="blog-main-tag">{tags.0}</span>
      <h1 class="h1 uppercase">{title}</h1>
      <span>{addedOn|date %b %d, %Y}</span>
    </div>
    <div class="blog-main-content">{body}</div>
  </div>
</section>

{.end}
<script src="../scripts/script.js"></script>